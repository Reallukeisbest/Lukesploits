-- Scripts:

local function QOYS_fake_script() -- TextButton.Script 
	local script = Instance.new('Script', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		local frame = script.Parent.Parent
		frame:TweenPosition(UDim2.new(-0.5, 0,0.319, 0))
		ScreenGui:Destroy()
		if game.PlaceId == 4919350725 then
			loadstring(game:HttpGet(('https://raw.githubusercontent.com/Reallukeisbest/Lukesploits/master/Ducky%20Simulator')))() 
		else
			if game.PlaceId == 4704006941 then
			loadstring(game:HttpGet(('https://raw.githubusercontent.com/Reallukeisbest/Lukesploits/master/Blade%20Throwing%20Simulator')))() 
			else
					if game.PlaceId == 4152904699 then
			loadstring(game:HttpGet(('https://raw.githubusercontent.com/Reallukeisbest/Lukesploits/master/Work%20at%20a%20Hotel!%20HOTEL%20HOTEL%20HOTEL!%20Job-Ifier')))()
			else
			loadstring(game:HttpGet(('https://raw.githubusercontent.com/Reallukeisbest/Lukesploits/master/Script%20Hub/Not%20Supported')))()
		end		
		end
		end
		end)
end
coroutine.wrap(QOYS_fake_script)()
local function DSYMMG_fake_script() -- Frame.Dragify 
	local script = Instance.new('LocalScript', Frame)

	local UIS = game:GetService("UserInputService")
	function dragify(Frame)
	    dragToggle = nil
	    local dragSpeed = 0
	    dragInput = nil
	    dragStart = nil
	    local dragPos = nil
	    function updateInput(input)
	        local Delta = input.Position - dragStart
	        local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
	        game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.25), {Position = Position}):Play()
	    end
	    Frame.InputBegan:Connect(function(input)
	        if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
	            dragToggle = true
	            dragStart = input.Position
	            startPos = Frame.Position
	            input.Changed:Connect(function()
	                if input.UserInputState == Enum.UserInputState.End then
	                    dragToggle = false
	                end
	            end)
	        end
	    end)
	    Frame.InputChanged:Connect(function(input)
	        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
	            dragInput = input
	        end
	    end)
	    game:GetService("UserInputService").InputChanged:Connect(function(input)
	        if input == dragInput and dragToggle then
	            updateInput(input)
	        end
	    end)
	end
	
	dragify(script.Parent)
end
coroutine.wrap(DSYMMG_fake_script)()
local function QMWJ_fake_script() -- TextButton_2.Script 
	local script = Instance.new('Script', TextButton_2)

	script.Parent.MouseButton1Click:Connect(function()
		local frame = script.Parent.Parent
		frame:TweenPosition(UDim2.new(-0.5, 0,0.319, 0))
		ScreenGui:Destroy()
		end)
end
coroutine.wrap(QMWJ_fake_script)()
