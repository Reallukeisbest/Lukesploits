local b = w:CreateFolder("AUTO") -- RANDOM NAME FOLDER

b:Label("Auto",Color3.fromRGB(38,38,38),Color3.fromRGB(0,216,111)) --BgColor,TextColor -- You can edit the Function's test

b:Button("Auto Sell On",function()-- here you have the button, that cool ! put the remote you have copied in that
    AS = true
    while AS == true do
    autoSell()
    wait(0.5)
end
end)
b:Button("Auto Sell Off",function()
    AS = false
    end)
b:Button("Auto Throw On (Dont run for too long or else u will get kicked)",function()
    AT = true
while AT == true do
    autoThrow()
    wait(0.5)
    end
end)
b:Button("Auto Throw Off",function()
    AT = false
    end)
