loadstring(game:HttpGet("https://pastebin.com/raw/xkrNeNaf"))()
wait (0.5)
AS = false
AT = false
EF = false
LG = false
RK = false

function autoThrow()
        
    local args = {
    [1] = "Throw",
    [2] = Vector3.new(game.Players.PlayerAdded:Connect(function(Player)
	Player.CharacterAdded:Connect(function(Character) end) end)),
    [3] = false,
}

game:GetService("ReplicatedStorage").RE:FireServer(unpack(args))
    end
function autoSell()
local args = {
    [1] = "SellPower",
    [2] = workspace.Ignore.Rings["21Sell"].Collider,
}
game:GetService("ReplicatedStorage").RF:InvokeServer(unpack(args))
end
